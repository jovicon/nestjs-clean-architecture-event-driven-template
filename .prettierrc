{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 120,
  "useTabs": false,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^@nestjs/(.*)$",
    "",
    "^@config/(.*)$",
    "^@shared/(.*)$",
    "",
    "^@modules/(.*)$",
    "",
    "^[./]"
  ]
}
